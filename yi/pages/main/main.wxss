.ls-tongji-1{height: 66rpx; line-height: 66rpx;  color: #543030; font-size: 28rpx;display: flex;border-bottom: 4rpx solid #f3f3f3;justify-content: center;}
.ls-tongji-1 text{width: 156rpx; text-align: center; }
.ls-tongji-2{height:420rpx;padding-bottom: 30rpx;border-bottom: 4rpx solid #f3f3f3;display: flex;justify-content: center; }
.ls-tongji-2-a{width: 156rpx; text-align: center;display: flex;justify-content:space-around;color: #543030;align-items:flex-end; font-size: 24rpx;}
.ls-tongji-2-a-view1 view:nth-of-type(2){width: 38rpx;height: 266rpx; border-radius: 10rpx;background: #90ea8d;}
.ls-tongji-2-a-view2 view:nth-of-type(2){width: 38rpx;height: 108rpx; border-radius: 10rpx;background: #ff7f80;}

.ls-tongji-y-c{font-size: 28rpx; margin: 30rpx auto 70rpx auto;display: flex; justify-content: center;}
.ls-tongji-y-c text:nth-of-type(1){position: relative;padding-left: 40rpx; padding-right: 20rpx;}
.ls-tongji-y-c text:nth-of-type(1)::before{position: absolute;content: '';width: 20rpx;height: 20rpx;background:#90ea8d;border-radius: 50%;left: 0;top: 10rpx; }
.ls-tongji-y-c text:nth-of-type(2){position: relative;padding-left: 40rpx;padding-right: 20rpx;}
.ls-tongji-y-c text:nth-of-type(2)::before{position: absolute;content: '';width: 20rpx;height: 20rpx;background:#ff7778;border-radius: 50%;left: 0;top: 10rpx; }
.ls-tongji-3{width: 640rpx; margin: auto; display: flex; justify-content: space-around;}

.ls-tongji-3-left{}
.ls-tongji-3-left-a{width: 300rpx; background: #f3f3f3; height: 80rpx; line-height: 80rpx; text-align: center; border-top-left-radius: 20rpx; border-top-right-radius: 20rpx;color: #afa4a4;font-size: 30rpx;}
.ls-tongji-3-left-b{width: 285rpx; border: 4px solid #f3f3f3;  text-align: center; border-bottom-left-radius: 20rpx; border-bottom-right-radius: 20rpx;font-size: 30rpx;height: 150rpx;color: #543030;} 
.ls-tongji-3-left-b text:nth-of-type(1){font-size: 120rpx;}
.ls-tongji-3-left-b text:nth-of-type(2){font-size: 30rpx;}
.ls-tongji-4{font-size: 28rpx;color: #afa4a4;width: 640rpx; text-align: center; line-height: 40rpx; margin-top: 20rpx;}

button::after{border: none;}