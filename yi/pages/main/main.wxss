@keyframes fromBack{
    0%{
       
        opacity: 0;transform-origin: 0 0;
    }
    50%{
      transform: scale(1.2);transform-origin: 10px 10px;
      opacity: 1;
    }
    100%{
       transform: scale(1);transform-origin: 5px 5px;
        opacity: 1;
    }
}
@keyframes fromBack2{
    0%{
       
        opacity: 0;
    }
    50%{
      transform: scale(1.2);
      opacity: 0;
    }
    100%{
       transform: scale(1);
        opacity: 1;
    }
}
